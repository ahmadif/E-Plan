{
  "openapi": "3.0.3",
  "info": {
    "title": "DXH REST API",
    "description": "Profile representing the Node Aggregate response.",
    "version": "0.02"
  },
  "servers": [
    {
      "url": "https://dxh-api.uat.misc.k8s.sickcn.net/"
    },
    {
      "url": "https://dxh-api.prod.misc.k8s.sickcn.net/"
    },
    {
      "url": "http://localhost:3000"
    }
  ],
  "paths": {
    "/product/{partNumber}/{languages}": {
      "get": {
        "description": "Get product nodes data per part number",
        "tags": [
          "Aggregated PIM Data"
        ],
        "parameters": [
          {
            "in": "path",
            "name": "partNumber",
            "required": true,
            "schema": {
              "type": "string"
            },
            "description": "The SICK product part number."
          },
          {
            "in": "path",
            "name": "languages",
            "required": true,
            "schema": {
              "type": "array",
              "items": {
                "type": "string"
              }
            },
            "description": "An array of languages for which to retrieve data."
          }
        ],
        "responses": {
          "200": {
            "description": "successful operation",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/NodeArray"
                }
              }
            }
          }
        }
      }
    },
    "/node/{nodeID}": {
      "get": {
        "description": "Retrieves Node item data by ID",
        "tags": [
          "Aggregated PIM Data"
        ],
        "parameters": [
          {
            "in": "path",
            "name": "nodeID",
            "required": true,
            "schema": {
              "type": "string"
            },
            "description": "4PM node ID suffixed by language code",
            "example": "p653505_de"
          },
          {
            "in": "query",
            "name": "fields",
            "schema": {
              "type": "array",
              "items": {
                "type": "string"
              }
            },
            "description": "needed Fields"
          }
        ],
        "responses": {
          "200": {
            "description": "successful operation",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Node"
                }
              }
            }
          }
        }
      }
    },
    "/childNodes/{parent}": {
      "get": {
        "description": "Retrieves Node children by parent ID",
        "tags": [
          "Aggregated PIM Data"
        ],
        "parameters": [
          {
            "in": "path",
            "name": "parent",
            "required": true,
            "schema": {
              "type": "string"
            },
            "description": "Parent 4PM node ID suffixed by language code"
          }
        ],
        "responses": {
          "200": {
            "description": "successful operation",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/NodeArray"
                }
              }
            }
          }
        }
      }
    },
    "/allNodes/{nodeID}": {
      "get": {
        "description": "Get all language specific Nodes based on nodeId",
        "tags": [
          "Aggregated PIM Data"
        ],
        "parameters": [
          {
            "in": "path",
            "name": "nodeID",
            "required": true,
            "schema": {
              "type": "string"
            },
            "description": "4PM node ID",
            "example": "p653505"
          },
          {
            "in": "query",
            "name": "fields",
            "schema": {
              "type": "array",
              "items": {
                "type": "string"
              }
            },
            "description": "needed fields"
          }
        ],
        "responses": {
          "200": {
            "description": "successful operation",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/NodeArray"
                }
              }
            }
          }
        }
      }
    },
    "/erpProduct/{partNumber}/{ssu}": {
      "get": {
        "description": "Retrieve SAP Data with PartNumber and Subsidiary",
        "tags": [
          "Aggregated ERP Data"
        ],
        "parameters": [
          {
            "in": "path",
            "name": "partNumber",
            "required": true,
            "schema": {
              "type": "string"
            },
            "description": "The SICK product part number."
          },
          {
            "in": "path",
            "name": "ssu",
            "required": true,
            "schema": {
              "type": "string"
            },
            "description": "SICK Subsidiary",
            "example": "SVD"
          }
        ],
        "responses": {
          "200": {
            "description": "successful operation",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ProductAggregate"
                }
              }
            }
          }
        }
      }
    },
    "/erpProducts/{partNumber}": {
      "get": {
        "description": "Retrieve SAP Data with PartNumber for all Subsidiaries",
        "tags": [
          "Aggregated ERP Data"
        ],
        "parameters": [
          {
            "in": "path",
            "name": "partNumber",
            "required": true,
            "schema": {
              "type": "string"
            },
            "description": "The SICK product part number."
          }
        ],
        "responses": {
          "200": {
            "description": "successful operation",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ProductAggregateArray"
                }
              }
            }
          }
        }
      }
    },
    "/channels": {
      "get": {
        "description": "Get ChannelConfig",
        "tags": [
          "Config"
        ],
        "parameters": [
          {
            "in": "query",
            "name": "ssu",
            "schema": {
              "type": "string"
            },
            "description": "get all channels based on ssu",
            "example": "SVD"
          },
          {
            "in": "query",
            "name": "language",
            "schema": {
              "type": "string"
            },
            "description": "get channels based on language",
            "example": "de"
          },
          {
            "in": "query",
            "name": "site",
            "schema": {
              "type": "string"
            },
            "description": "get channels based on site",
            "example": "de"
          },
          {
            "in": "query",
            "name": "all",
            "schema": {
              "type": "boolean"
            },
            "description": "get all channels in database"
          }
        ],
        "responses": {
          "200": {
            "description": "successful operation",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Channel"
                }
              }
            }
          }
        }
      }
    }
  },
